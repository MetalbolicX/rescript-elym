{
  "name": "resforge",
  "description": "ResForge is package to manipulate the DOM to make front-end development easier and faster rather than writting JavaScript bindings manually.",
  "version": "1.0.0",
  "scripts": {
    "res:bundle": "deno bundle ./example/Index.res.mjs -o ./example/main.js",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w",
    "res:test": "retest --with-dom test/**/*.res.mjs",
    "docs": "docsify serve docs"
  },
  "keywords": [
    "rescript"
  ],
  "files": ["src"],
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/resforge.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/resforge/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "1.6.1",
    "rescript": "11.1.4"
  },
  "devDependencies": {
    "rescript-test": "^7.0.1"
  }
}
