{
  "name": "rescript-elym",
  "description": "A ReScript v11 library for creating and manipulating HTML elements using a syntax similar to d3.js or jQuery.",
  "version": "1.1.0",
  "scripts": {
    "rd:build": "rolldown ./example/Index.res.mjs --file ./dist/main.js --format es",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w",
    "res:test": "retest --with-dom test/**/*.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "rescript"
  ],
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/rescript-elym.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/rescript-elym/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "1.6.1",
    "rescript": "11.1.4",
    "vitepress-plugin-group-icons": "^1.5.5"
  },
  "devDependencies": {
    "rescript-test": "^7.0.1",
    "rolldown": "^1.0.0-beta.9",
    "vitepress": "^1.6.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
