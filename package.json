{
  "name": "rescript-elym",
  "description": "A ReScript v11 module that allows you to manipulate the DOM and change attributes, styles, properties, HTML or text using a d3.js like syntax.",
  "version": "1.1.0",
  "scripts": {
    "rd:build": "rolldown ./example/Index.res.mjs --file ./example/main.js --format es",
    "res:build": "rescript",
    "res:clean": "rescript clean",
    "res:dev": "rescript -w",
    "res:test": "retest --with-dom test/**/*.res.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "rescript"
  ],
  "author": {
    "name": "José Martínez Santana"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MetalbolicX/rescript-elym.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/rescript-elym/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@rescript/core": "1.6.1",
    "rescript": "11.1.4"
  },
  "devDependencies": {
    "rescript-test": "^7.0.1",
    "rolldown": "^1.0.0-beta.9",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.5.5"
  }
}